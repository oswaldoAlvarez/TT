{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2021"
    ],
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-native"
    ]
  },
  "include": [
    "app",
    "src",
    "*.ts",
    "*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
